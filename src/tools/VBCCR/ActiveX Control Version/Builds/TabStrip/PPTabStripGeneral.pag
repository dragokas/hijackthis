Attribute VB_Name = "PPTabStripGeneral"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = True
Option Explicit
Private ImageListEnumerated As Boolean
Private FreezeChanged As Boolean

Private Sub PropertyPage_Initialize()
Call ComCtlsShowAllUIStates(PropertyPage.hWnd)
Call ComCtlsPPInitComboMousePointer(ComboMousePointer)
With ComboImageList
.AddItem "(None)"
.ListIndex = 0
End With
With ComboPlacement
.AddItem TbsPlacementTop & " - Top"
.ItemData(.NewIndex) = TbsPlacementTop
.AddItem TbsPlacementBottom & " - Bottom"
.ItemData(.NewIndex) = TbsPlacementBottom
.AddItem TbsPlacementLeft & " - Left"
.ItemData(.NewIndex) = TbsPlacementLeft
.AddItem TbsPlacementRight & " - Right"
.ItemData(.NewIndex) = TbsPlacementRight
End With
With ComboStyle
.AddItem TbsStyleTabs & " - Tabs"
.ItemData(.NewIndex) = TbsStyleTabs
.AddItem TbsStyleButtons & " - Buttons"
.ItemData(.NewIndex) = TbsStyleButtons
.AddItem TbsStyleFlatButtons & " - FlatButtons"
.ItemData(.NewIndex) = TbsStyleFlatButtons
End With
With ComboTabStyle
.AddItem TbsTabStyleStandard & " - Standard"
.ItemData(.NewIndex) = TbsTabStyleStandard
.AddItem TbsTabStyleOpposite & " - Opposite"
.ItemData(.NewIndex) = TbsTabStyleOpposite
End With
With ComboTabWidthStyle
.AddItem TbsTabWidthStyleJustified & " - Justified"
.ItemData(.NewIndex) = TbsTabWidthStyleJustified
.AddItem TbsTabWidthStyleNonJustified & " - NonJustified"
.ItemData(.NewIndex) = TbsTabWidthStyleNonJustified
.AddItem TbsTabWidthStyleFixed & " - Fixed"
.ItemData(.NewIndex) = TbsTabWidthStyleFixed
End With
With ComboTabAlignment
.AddItem TbsTabAlignmentStandard & " - Standard"
.ItemData(.NewIndex) = TbsTabAlignmentStandard
.AddItem TbsTabAlignmentImageLeft & " - ImageLeft"
.ItemData(.NewIndex) = TbsTabAlignmentImageLeft
.AddItem TbsTabAlignmentImageCaptionLeft & " - ImageCaptionLeft"
.ItemData(.NewIndex) = TbsTabAlignmentImageCaptionLeft
End With
With ComboDrawMode
.AddItem TbsDrawModeNormal & " - Normal"
.ItemData(.NewIndex) = TbsDrawModeNormal
.AddItem TbsDrawModeOwnerDrawFixed & " - OwnerDrawFixed"
.ItemData(.NewIndex) = TbsDrawModeOwnerDrawFixed
End With
End Sub

Private Sub PropertyPage_ApplyChanges()
On Error Resume Next
PropertyPage.ValidateControls
If Err.Number = 380 Then Exit Sub
On Error GoTo 0
With PropertyPage.SelectedControls(0)
.Enabled = CBool(CheckEnabled.Value = vbChecked)
.VisualStyles = CBool(CheckVisualStyles.Value = vbChecked)
.DoubleBuffer = CBool(CheckDoubleBuffer.Value = vbChecked)
.MousePointer = ComboMousePointer.ItemData(ComboMousePointer.ListIndex)
.ImageList = ComboImageList.List(ComboImageList.ListIndex)
.Placement = ComboPlacement.ItemData(ComboPlacement.ListIndex)
.Style = ComboStyle.ItemData(ComboStyle.ListIndex)
.TabStyle = ComboTabStyle.ItemData(ComboTabStyle.ListIndex)
.TabWidthStyle = ComboTabWidthStyle.ItemData(ComboTabWidthStyle.ListIndex)
.TabFixedWidth = CSng(TextTabFixedWidth.Text)
.TabFixedHeight = CSng(TextTabFixedHeight.Text)
.TabMinWidth = CSng(TextTabMinWidth.Text)
.TabAlignment = ComboTabAlignment.ItemData(ComboTabAlignment.ListIndex)
.DrawMode = ComboDrawMode.ItemData(ComboDrawMode.ListIndex)
.MouseTrack = CBool(CheckMouseTrack.Value = vbChecked)
.MultiRow = CBool(CheckMultiRow.Value = vbChecked)
.MultiSelect = CBool(CheckMultiSelect.Value = vbChecked)
.HotTracking = CBool(CheckHotTracking.Value = vbChecked)
.Separators = CBool(CheckSeparators.Value = vbChecked)
.ShowTips = CBool(CheckShowTips.Value = vbChecked)
.TabScrollWheel = CBool(CheckTabScrollWheel.Value = vbChecked)
End With
Call PropertyPage_SelectionChanged
End Sub

Private Sub PropertyPage_SelectionChanged()
Dim i As Long
FreezeChanged = True
With PropertyPage.SelectedControls(0)
CheckEnabled.Value = IIf(.Enabled = True, vbChecked, vbUnchecked)
CheckVisualStyles.Value = IIf(.VisualStyles = True, vbChecked, vbUnchecked)
CheckDoubleBuffer.Value = IIf(.DoubleBuffer = True, vbChecked, vbUnchecked)
For i = 0 To ComboMousePointer.ListCount - 1
    If ComboMousePointer.ItemData(i) = .MousePointer Then
        ComboMousePointer.ListIndex = i
        Exit For
    End If
Next i
If ImageListEnumerated = False Then
    Dim ControlEnum As Object
    For Each ControlEnum In .ControlsEnum
        If TypeName(ControlEnum) = "ImageList" Then
            ComboImageList.AddItem ProperControlName(ControlEnum)
        End If
    Next ControlEnum
    ImageListEnumerated = True
End If
For i = 0 To ComboImageList.ListCount - 1
    If ComboImageList.List(i) = .ImageList Then
        ComboImageList.ListIndex = i
        Exit For
    End If
Next i
For i = 0 To ComboPlacement.ListCount - 1
    If ComboPlacement.ItemData(i) = .Placement Then
        ComboPlacement.ListIndex = i
        Exit For
    End If
Next i
For i = 0 To ComboStyle.ListCount - 1
    If ComboStyle.ItemData(i) = .Style Then
        ComboStyle.ListIndex = i
        Exit For
    End If
Next i
For i = 0 To ComboTabStyle.ListCount - 1
    If ComboTabStyle.ItemData(i) = .TabStyle Then
        ComboTabStyle.ListIndex = i
        Exit For
    End If
Next i
For i = 0 To ComboTabWidthStyle.ListCount - 1
    If ComboTabWidthStyle.ItemData(i) = .TabWidthStyle Then
        ComboTabWidthStyle.ListIndex = i
        Exit For
    End If
Next i
TextTabFixedWidth.Text = CStr(.TabFixedWidth)
TextTabFixedHeight.Text = CStr(.TabFixedHeight)
TextTabMinWidth.Text = CStr(.TabMinWidth)
For i = 0 To ComboTabAlignment.ListCount - 1
    If ComboTabAlignment.ItemData(i) = .TabAlignment Then
        ComboTabAlignment.ListIndex = i
        Exit For
    End If
Next i
For i = 0 To ComboDrawMode.ListCount - 1
    If ComboDrawMode.ItemData(i) = .DrawMode Then
        ComboDrawMode.ListIndex = i
        Exit For
    End If
Next i
CheckMouseTrack.Value = IIf(.MouseTrack = True, vbChecked, vbUnchecked)
CheckMultiRow.Value = IIf(.MultiRow = True, vbChecked, vbUnchecked)
CheckMultiSelect.Value = IIf(.MultiSelect = True, vbChecked, vbUnchecked)
CheckHotTracking.Value = IIf(.HotTracking = True, vbChecked, vbUnchecked)
CheckSeparators.Value = IIf(.Separators = True, vbChecked, vbUnchecked)
CheckShowTips.Value = IIf(.ShowTips = True, vbChecked, vbUnchecked)
CheckTabScrollWheel.Value = IIf(.TabScrollWheel = True, vbChecked, vbUnchecked)
End With
FreezeChanged = False
End Sub

