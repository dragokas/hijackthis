Private Sub TextTabFixedWidth_GotFocus()
If Not TextTabFixedWidth.SelStart = 0 Then Exit Sub
TextTabFixedWidth.SelLength = Len(TextTabFixedWidth.Text)
End Sub

Private Sub TextTabFixedWidth_LostFocus()
TextTabFixedWidth.SelStart = 0
End Sub

Private Sub TextTabFixedWidth_Change()
If FreezeChanged = True Then Exit Sub
If TextTabFixedWidth.Text = vbNullString Then
    TextTabFixedWidth.Text = PropertyPage.SelectedControls(0).TabFixedWidth
    TextTabFixedWidth.SelStart = 0
    TextTabFixedWidth.SelLength = Len(TextTabFixedWidth.Text)
End If
PropertyPage.Changed = True
End Sub

Private Sub TextTabFixedWidth_Validate(Cancel As Boolean)
If Not IsNumeric(TextTabFixedWidth.Text) Then
    MsgBox "Invalid property value", vbCritical + vbOKOnly
    TextTabFixedWidth.Text = PropertyPage.SelectedControls(0).TabFixedWidth
    TextTabFixedWidth.SetFocus
    Cancel = True
Else
    Dim SngValue As Single
    On Error Resume Next
    SngValue = CSng(TextTabFixedWidth.Text)
    Cancel = CBool(Err.Number <> 0)
    On Error GoTo 0
    If SngValue < 0 Then Cancel = True
    If Cancel = False Then
        TextTabFixedWidth.Text = SngValue
    Else
        MsgBox "Invalid property value", vbCritical + vbOKOnly
        TextTabFixedWidth.Text = PropertyPage.SelectedControls(0).TabFixedWidth
        TextTabFixedWidth.SetFocus
    End If
End If
End Sub

